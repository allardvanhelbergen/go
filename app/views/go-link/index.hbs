<h1 class="page-header">All Go Links</h1>
<table class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>Short URI</th>
            <th class="col-md-3">Long URI</th>
            <th>Created</th>
            <th>Owner</th>
        </tr>
    </thead>
    <tbody>
        {{#each goLinks}}
            <tr>
                <td><a href="/_api/go-link/{{id}}">{{id}}</a></td>
                <td>{{shortUri}}</td>
                <td class="table-cell--ellipsis">{{longUri}}</td>
                <td>{{created}}</td>
                <td>{{ownerId}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
